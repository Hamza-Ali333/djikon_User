<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.djikon">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:resizeableActivity="false"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity android:name=".UserProfileActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            />
        <activity android:name=".PurchaseServiceActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar"
            />
        <activity android:name=".BookArtistActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".ServiceDetailActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".ResetPasswordActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".DjPrpfileActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".BookingHistoryActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".ProfileSettingActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".PaymentMethodActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".BookingPaymentMethodActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".SignInActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".RegistrationActivity"
            android:screenOrientation="portrait"
            android:resizeableActivity="false"
            />
        <activity android:name=".SplashScreenActivity"
            android:resizeableActivity="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>